{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "importHelpers": true,
    "isolatedModules": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "../../../lib/test/examples/webpack",
    "paths": {
      "tsx-control-statements/*": [
        "../../../src/*"
      ],
      "tsx-control-statements": [
        "../../../src/index.ts"
      ]
    }
  },
  "references": [
    {
      "path": "../../tsconfig.json"
    },
    {
      "path": "../../../src/tsconfig.json"
    }
  ],
  "files": ["index.tsx"]
}
