{
  "compilerOptions": {
    "allowJs": false,
    "alwaysStrict": false,
    "declaration": true,
    "target": "es6",
    "lib": [
      "dom",
      "es6",
      "es2017",
      "es2017.object",
      "es2016"
    ],
    "module": "commonjs",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "downlevelIteration": true,
    "listEmittedFiles": true,
    "newLine": "LF",
    "removeComments": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "strict": false,
    "pretty": true,
    "outDir": "lib",
    "baseUrl": "./",
    "jsx": "react",
    "composite": true
  },
  
  "references": [
    {
      "path": "./test/examples/webpack/tsconfig.json"
    },
    {
      "path": "./test/tsconfig.json"
    },
    {
      "path": "./src/tsconfig.json"
    }
  ],
  "files": []
}
