{
  "compilerOptions": {
    "allowJs": false,
    "alwaysStrict": false,
    "declaration": true,
    "target": "es6",
    "lib": [
      "dom",
      "ES7",
      "ES6",
      "ESNext",
      "ES2020",
      "ES2019",
      "ES2018",
      "ES2017",
      "ES2016"
    ],
    "module": "commonjs",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "downlevelIteration": true,
    "listEmittedFiles": true,
    "newLine": "LF",
    "removeComments": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "strict": false,
    "pretty": true,
    "outDir": "lib",
    "baseUrl": "./",
    "jsx": "react",
    "composite": true
  },
  
  "references": [
    {
      "path": "./test/tsconfig.json"
    },
    {
      "path": "./src/tsconfig.json"
    }
  ],
  "files": []
}
