  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "init": "./scripts/init.fish",
    "//publish": "yarn build && npm login && npm publish",
    "build": "./scripts/build.fish",
    "build:watch": "yarn build -- --watch",
    "test": "yarn test:compile && yarn test:run-compiled",
    "test:ci": "./runtests.sh",
    "test:compile": "cd ../test && ./compile-cases.sh && cd ..",
    "test:run-compiled": "mocha ../test/helpers/browser-env.js \\\"test/**/*.spec.js\\\"",
    "test:coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:html-report": "nyc report --reporter=html",
    "postversion": "./scripts/release.fish $npm_package_version && echo \"Successfully released version $npm_package_version!\"",
    "release": "yarn build && yarn version --patch --non-interactive"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-jsx-control-statements": "^4.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "fuse-box": "^3.7.1",
    "jsdom": "^16.4.0",
    "jsx-control-statements": "https://github.com/3fv/jsx-control-statements.git",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "shelljs": "^0.8.4",
    "ts-node": "^9.0.0",
    "typescript": "4.1.0-beta"
  },
  "peerDependencies": {
    "typescript": ">=4.0.3"
  }
}
